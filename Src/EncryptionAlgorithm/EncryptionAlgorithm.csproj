<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <AssemblyName>Lindexi.Src.EncryptionAlgorithm</AssemblyName>
    <RootNamespace>Lindexi.Src.EncryptionAlgorithm</RootNamespace>
    <Nullable>enable</Nullable>
